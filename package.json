{
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./build",
    "build": "npm run build:clean && npm run compile && node ./scripts/prepare-package-json.js",
    "test": "vitest run src",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "build"
  ],
  "name": "@krakaw/wallet-interface",
  "version": "0.0.7",
  "description": "Tari Wallet API",
  "main": "./build/cjs/lib.js",
  "module": "./build/esm/lib.js",
  "types": "./build/types/lib.d.ts",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "google-protobuf": "^3.21.4",
    "prettier": "^3.5.3",
    "protoc-gen-ts": "^0.8.7",
    "ts-proto": "^2.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@grpc/grpc-js": "^1.13.3",
    "base58-monero": "^0.0.5",
    "bs58": "^6.0.0",
    "long": "^5.3.1"
  },
  "exports": {
    ".": {
      "types": "./build/types/lib.d.ts",
      "require": "./build/cjs/lib.js",
      "import": "./build/esm/lib.js"
    },
    "./TariWalletCliClient": {
      "types": "./build/types/TariWalletCliClient.d.ts",
      "require": "./build/cjs/TariWalletCliClient.js",
      "import": "./build/esm/TariWalletCliClient.js"
    },
    "./TariWalletGrpcClient": {
      "types": "./build/types/TariWalletGrpcClient.d.ts",
      "require": "./build/cjs/TariWalletGrpcClient.js",
      "import": "./build/esm/TariWalletGrpcClient.js"
    },
    "./TariBaseNodeGrpcClient": {
      "types": "./build/types/TariBaseNodeGrpcClient.d.ts",
      "require": "./build/cjs/TariBaseNodeGrpcClient.js",
      "import": "./build/esm/TariBaseNodeGrpcClient.js"
    }
  }
}
